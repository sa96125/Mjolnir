datasource db {
  provider = "postgresql"
  url = env("VITE_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

model User {
  uid       String   @id @default(cuid())
  createdAt DateTime @default(now())
  email     String   @unique
  password  String
  name      String?
}